  "description": "ACM TTU Website",
  "scripts": {
    "app": "NODE_ENV=dev CLIENT='' node ./bin/www",
    "app-prod": "NODE_ENV=prod CLIENT='' node ./bin/www",
    "api": "nodemon",
    "api-test": "./node_modules/.bin/mocha --reporter spec --timeout 120000000 --exit",
    "client": "ng serve --aot",
    "client-test": "ng test",
    "start": "NODE_ENV=dev CLIENT=http://localhost:4200 node ./bin/www",
    "build": "ng build --prod --aot",
    "test": "ng e2e && npm run client-test && npm run api-test",
    "docker": "docker build -t acmtexastech . && docker run -p 80:8080 acmtexastech",
    "docs": "./node_modules/.bin/esdoc"
